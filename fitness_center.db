
CREATE DATABASE fitness_center_db;

USE fitness_center_db;

CREATE TABLE Members (
id INT PRIMARY KEY,
member_name BOOLEAN NOT NULL,
age INT,
trainer_id INT,
FOREIGN KEY (trainer_id) REFERENCES Trainer(id)
);

